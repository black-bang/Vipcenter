{
  "name": "ReactWebpack",
  "version": "1.0.0",
  "description": "快速搭建基于webpack的React开发环境",
  "main": "./build/PureEntry.js",
  "scripts": {
    "setDev": "set NODE_ENV=development",
    "setProd": "set NODE_ENV=production",
    "removeLib": "rimraf ./lib",
    "webpackDev": "webpack-dev-server --config ./webpack.config/webpack.dev.config.js",
    "webpackWatch":"webpack --config ./webpack.config/webpack.watch.config.js",
    "webpackAssets": "webpack --config ./webpack.config/webpack.assets.config.js",
    "dev": "npm run setDev && npm run webpackDev",
    "watch":"npm run setDev && npm run webpackWatch",
    "assets": "npm run removeLib &&npm run setProd && npm run webpackAssets"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "babel": {
    "presets": [
      "./webpack.config/.babelrc.js"
    ]
  },
  "author": "ysc",
  "license": "ISC",
  "devDependencies": {
    "antd-mobile": "^2.1.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-mobx": "^1.0.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "d3-format": "^1.3.2",
    "date-format": "^1.2.0",
    "draft-js": "^0.10.5",
    "es6-promise": "^4.2.2",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "1.1.5",
    "glob": "^7.1.3",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "js-cookie": "^2.2.0",
    "json-loader": "^0.5.7",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "moment": "^2.22.2",
    "node-sass": "^4.7.2",
    "open-browser-webpack-plugin": "0.0.5",
    "pubsub-js": "^1.6.1",
    "react": "^16.4.1",
    "react-css-modules": "^4.7.7",
    "react-dom": "^16.4.1",
    "react-draft-wysiwyg": "^1.12.13",
    "react-hammerjs": "^1.0.1",
    "react-number-format": "^3.4.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^0.6.2",
    "validator": "^10.4.0",
    "vconsole": "^3.2.0",
    "vconsole-webpack-plugin": "^1.4.0",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-module-hot-accept": "^1.0.5",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "draft-js-plugins-editor": "^2.1.1"
  }
}
